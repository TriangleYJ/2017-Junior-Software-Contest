<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="Filtering Setting">
    <EditTextPreference
        android:title="Name"
        android:key="username"
        android:summary="Modify your username" />
    <com.yjprojects.jsctest2.SeekBarPreference
        android:defaultValue="50"
        android:dialogMessage="무늬의 밀집도를 조정할 수 있습니다. \n밀도를 과도하게 높일 시 예기치 않게 종료될 수 있습니다."
        android:key="density"
        android:max="100"
        android:summary="Adjust the density of Pattern"
        android:title="Density" />
    <com.yjprojects.jsctest2.SeekBarPreference
        android:defaultValue="750"
        android:dialogMessage="이미지의 화질를 조정할 수 있습니다. \n화질을 과도하게 높일 시 예기치 않게 종료될 수 있습니다."
        android:key="quality"
        android:max="1500"
        android:summary="Adjust the quality of Filtered Image"
        android:title="Quality" />
    <ListPreference
        android:title="Color"
        android:summary="Modify your color-blind type"
        android:key="mode"
        android:defaultValue="1"
        android:entries="@array/listArray"
        android:entryValues="@array/listValues" />

    </PreferenceCategory>
</PreferenceScreen>